                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="Stats" style="flex-grow: 0; flex-shrink: 0; flex-direction: column; align-items: auto; width: 70%; height: 80%; left: auto; right: 15%; top: -26%; align-self: auto;">
                <ui:VisualElement name="MeleeEnemy" style="flex-grow: 0; flex-shrink: 0; width: 100%; height: 25%; min-width: auto; min-height: auto; flex-direction: row;">
                    <ui:Label tabindex="-1" text="Melees killed......................" parse-escape-sequences="true" display-tooltip-when-elided="true" name="StatName" style="height: 90%; width: 80%; color: rgb(255, 255, 255); -unity-text-align: middle-left; font-size: 35%;" />
                    <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="MeleeValue" style="height: 90%; width: 20%; -unity-text-align: middle-right; color: rgb(255, 255, 255); font-size: 35%;" />
                </ui:VisualElement>
                <ui:VisualElement name="FlyingEnemy" style="flex-grow: 0; flex-shrink: 0; width: 100%; height: 25%; min-width: auto; min-height: auto; flex-direction: row;">
