                <ui:Label tabindex="-1" text="value" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ScoreAmount" style="width: 50%; height: 100%; -unity-text-align: middle-center; font-size: 50%; color: rgb(255, 255, 255);" />
