                    <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="FlyingValue" style="height: 90%; width: 20%; -unity-text-align: middle-right; color: rgb(255, 255, 255); font-size: 35%;" />
                </ui:VisualElement>
                <ui:VisualElement name="KamikazeEnemy" style="flex-grow: 0; flex-shrink: 0; width: 100%; height: 25%; min-width: auto; min-height: auto; flex-direction: row;">
                    <ui:Label tabindex="-1" text="Kamikazes killed................" parse-escape-sequences="true" display-tooltip-when-elided="true" name="StatName" style="height: 90%; width: 80%; color: rgb(255, 255, 255); -unity-text-align: middle-left; font-size: 35%;" />
