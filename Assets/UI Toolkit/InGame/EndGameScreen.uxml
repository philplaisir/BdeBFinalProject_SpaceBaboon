            <ui:VisualElement name="Score" style="flex-grow: 0; flex-shrink: 0; flex-direction: row; align-items: auto; width: 70%; height: 20%; left: auto; right: 15%; top: -18%; border-left-color: rgb(128, 128, 128); border-right-color: rgb(128, 128, 128); border-top-color: rgb(128, 128, 128); border-bottom-color: rgb(128, 128, 128); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; border-top-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; border-bottom-left-radius: 10px;">
