            <ui:VisualElement name="Title" style="flex-grow: 0; flex-shrink: 0; flex-direction: column; align-items: auto; width: 70%; height: 20%; left: auto; right: 15%; top: -37%;">
                <ui:Label tabindex="-1" text="Game over !" parse-escape-sequences="true" display-tooltip-when-elided="true" name="YouLostText" enable-rich-text="true" style="width: 100%; height: 100%; -unity-text-align: middle-center; font-size: 80%; color: rgb(255, 255, 255); top: -70%;" />
                <ui:VisualElement name="TimeSurvived" style="flex-grow: 0; flex-shrink: 0; width: 100%; height: 80%; flex-direction: row; top: -48px;">
