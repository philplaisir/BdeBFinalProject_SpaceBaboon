<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="EndGameScreen" style="flex-grow: 0; align-items: auto; width: 80%; height: 80%; flex-shrink: 0; left: 10%; top: 10%; background-color: rgba(0, 0, 0, 0.85); border-top-left-radius: 10%; border-top-right-radius: 10%; border-bottom-right-radius: 10%; border-bottom-left-radius: 10%; transition-property: background-color; transition-duration: 5s; transition-timing-function: ease-in; display: none;">
